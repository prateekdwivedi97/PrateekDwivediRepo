<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean class="com.capg.bean.Employ" id="cmp">
    <property name="empId" value="100"/>
    <property name="empName" value="Hero"/>
    <property name="salary" value="100000000"/>
    <property name="businUnit" value="Manufacturing-Unit1"/>
    <property name="age" value="50"/>
    </bean>
     <bean class="com.capg.bean.SBU" id="sbu">
    <property name="sbuId" value="100"></property>
    <property name="sbuName" value="Raj"></property>
    <property name="sbuHead" value="Product Engineering Services"></property>
    </bean>
</beans>